#import Win32 = (*) from win32;

#export {
    ausgabe
}

Optional :: struktur {
    value : *void;
}

ausgabe :: proc(fmt: string) {
    mit Win32;

    handle := get_std_handle(cast(u32)Device_Type.STD_OUTPUT_HANDLE);
    chars_written : u32;
    write_console(handle, cast(* void)fmt, cast(u32)fmt.size, @chars_written, 0);
}

