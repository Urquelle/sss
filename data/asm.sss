#lade "W:/sss/data/expr.sss";

Vec3 :: struktur {
    x, y, z : u32;
}

master :: proc() -> u32 {
    a : s32 = 5;
    b : u32;
    c : [3][3] u32;
    v : Vec3;

    falls a < 10 {
        test(*v.y);
    } oder {
        res 33;
    }

    b = index(1);
    c[index(2)][b] = 15;

    res c[2][b];
}

test :: proc(x : *u32) {
    @x = 25;
}

index :: proc(i : u32) -> u32 {
    falls i == 1 {
        res 1;
    } oder {
        res 2;
    }
}

