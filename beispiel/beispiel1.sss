#load "W:/sss/data/expr.sss";

Vec3 :: struktur {
    x, y, z : u32;
}

master :: proc() -> u32 {
    a : s32 = 5;
    b : u32;
    c : [3][3] u32;
    v : Vec3;

    falls a < 10 {
        test(*v.y);
    } oder {
        res 33;
    }

    b = index();
    c[b][b] = 15;

    res c[b][b];
}

test :: proc(x : *u32) {
    @x = 25;
}

index :: proc() -> u32 {
    res 1;
}

